{
  "name": "ilc-server-sdk",
  "version": "1.0.0",
  "description": "SDK for server side communication with Isomorphic Layout Composer",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsc",
    "test": "mocha -r ts-node/register **/*.spec.ts",
    "coverage": "nyc npm run test",
    "format": "prettier --write \"src/**/*.ts\" \"src/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"src/*.ts\"",
    "lint": "tslint -p tsconfig.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/namecheap/ilc-server-sdk.git"
  },
  "author": "Namecheap, Inc",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/namecheap/ilc-server-sdk/issues"
  },
  "homepage": "https://github.com/namecheap/ilc-server-sdk#readme",
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/lodash.merge": "^4.6.6",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.13.4",
    "@types/sinon": "^9.0.0",
    "chai": "^4.2.0",
    "lodash.merge": "^4.6.2",
    "mocha": "^7.1.2",
    "mock-http": "^1.1.0",
    "nyc": "^15.0.1",
    "prettier": "^2.0.5",
    "sinon": "^9.0.2",
    "ts-node": "^8.10.1",
    "tslint": "^6.1.2",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "@types/url-join": "^4.0.0",
    "url-join": "^4.0.1"
  }
}
